<h1><%= full_name(@employee)  %></h1>

<%= image_tag @employee.img_url %><br>
Alias: <%= @employee.alias %><br>
Title: <%= @employee.title %><br>
Office: <%= @employee.office %><br>
Dog: <%=link_to @employee.dog.name, dog_path(@employee.dog) %><br>

<%= button_to "Edit Employee", edit_employee_path, method: :get %><br>

<%= button_to "Delete Employee", @employee, method: :delete %><br>